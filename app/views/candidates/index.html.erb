<h1>Candidate List</h1>

<%= link_to "New Candidate", new_candidate_path %>

<table>
    <thead>
        <tr>
            <td>Name</td>
            <td>Party</td>
            <td>Politics</td>
            <td>Votes</td>
        </tr>
    </thead>
    <tbody>
        <% @candidates.each do |candidate| %>
        <tr>
            <td><%= candidate.name %>(age:<%= candidate.age %>)</td>
            <td><%= candidate.party %></td>
            <td><%= candidate.politics %></td>
            <td><%= candidate.votes %></td>
        </tr>
        <% end %>
    </tbody>
</table>



